<div class="d-flex justify-content-between align-items-center p-3 m-3">
  <p class="fw-bold text-primary"><%=User.display_time%></p>
  <h1 class="text-center text-primary">Emergency Room Queue</h1>
  <h2 class="text-center text-primary border border-primary rounded p-3"><%=PatientRequest.get_top.first_name%>, you are next!</h2>
  <% %>
</div>
<div class="container">
  <div class="row d-flex">
    <% @patient_requests.each do |patient_request| %>
      <%= render patient_request %>
    <% end %>
    <div class="d-flex justify-content-center m-3">
      <%= will_paginate @forms, renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
    </div>
  </div>
</div>
